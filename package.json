{
  "name": "mfgames-culture-es6",
  "version": "0.0.0",
  "main": "lib/init.js",
  "devDependencies": {
    "babel-cli": "^6.1.4",
    "babel-plugin-transform-es2015-modules-amd": "^6.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.1.4",
    "babel-preset-es2015": "^6.1.4",
    "jasmine-es6": "0.0.18",
    "mfgames-culture-data": "^0.0.0",
    "mocha": "^2.3.3"
  },
  "scripts": {
    "test": "jasmine es6/spec/*-spec.js",
    "build:amd": "babel --plugins transform-es2015-modules-amd -d amd es6",
    "build:commonjs": "babel --plugins transform-es2015-modules-commonjs -d commonjs es6",
    "build": "npm run build:amd && npm run build:commonjs"
  },
  "dependencies": {
    "babel-core": "^6.1.4"
  }
}
